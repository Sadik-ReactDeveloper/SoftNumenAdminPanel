<DropdownWarrantyType>
<dropdown>
<label>WarrantyType</label>
<name>Partner Code</name>
<option value="Standard">Standard</option>
<option value="Extended">Extended</option>
</dropdown>
</DropdownWarrantyType>

 <input>
		<label>Warranty Start Date</label>
		<type type="date">text</type>
		<name>StartDate</name>
	</input>
 <input>
		<label>Warranty End Date</label>
		<type type="date">text</type>
		<name>EndDate</name>
	</input>

 <input>
		<label>Days to Expire</label>
		<type type="date">text</type>
		<name>DaystoExpire</name>
	</input>


<DropdownDamageType>
<dropdown>
<label>Damage Type</label>
<name>DamageType</name>
<option value="Regular Inspection">Regular Inspection</option>
<option value="Scratches">Scratches</option>
<option value="Dents">Dents</option>
<option value="Cracks">Cracks</option>
<option value="Fracture">Fracture</option>
<option value="Breakdown">Breakdown</option>
<option value="Wiring">Wiring</option>
<option value="Glasswork">Glasswork</option>
<option value="crack">crack</option>
<option value="Engine Failure">Engine Failure</option>
</dropdown>
</DropdownDamageType>
<input>
		<label>Days Of Purchase</label>
		<type type="date">text</type>
		<name>DaystoExpire</name>
	</input>
<input>
