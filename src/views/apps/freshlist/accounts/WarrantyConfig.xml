	<CheckBox>
        <h3>Covered Under Warranty</h3>
		<input>
			<label>Yes</label>
			<type type="checkbox">text</type>
			<name>Yes</name>
		</input>
		<input>
			<label>No</label>
			<type type="checkbox">text</type>
			<name>No</name>
		</input>
